{
  "name": "react-dapp-requirements",
  "version": "0.1.1",
  "description": "(https://github.com/centrifuge/precise-proofs)",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "build:commonjs": "rollup ./src/index.js --file ./lib/precise-proof.js --format cjs",
    "build:es": "rollup ./src/index.js --file ./es/precise-proof.js --format es",
    "build:umd": "rollup ./src/index.js --file ./dist/precise-proof.js --format umd npm install babel-preset-env --save-dev'",
    "build": "rollup -c",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Centri/precise-proofs-js.git"
  },
  "keywords": [
    "precise",
    "proofs"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CentrifugeInc/precise-proofs-js/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/"
    ]
  },
  "homepage": "https://github.com/CentrifugeInc/precise-proofs-js#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babelrc-rollup": "^3.0.0",
    "jest": "^23.4.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "regenerator-runtime": "^0.12.0",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-visualizer": "^0.9.0",
    "standard-version": "^4.4.0"
  }
}
